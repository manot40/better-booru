{
  "compilerOptions": {
    "types": ["@types/bun"],
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": "./src",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
